{
  "name": "emerson-eims",
  "version": "1.0.0",
  "description": "Emerson EIMS - Energy Infrastructure Management System with WordPress Integration",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "export": "next build && next export",
    "analyze": "next build --analyze",
    "analyze:bundle": "npm run build && npx @next/bundle-analyzer",
    "performance": "npm run build && lighthouse http://localhost:3000 --output=json --output-path=./performance-report.json",
    "clean": "echo Cleaning build artifacts...",
    "clean:all": "echo Cleaning all...",
    "prebuild": "echo Running pre-build checks...",
    "build:prod": "npm run clean && npm run build",
    "build:analyze": "npm run analyze",
    "deploy": "powershell -ExecutionPolicy Bypass -File scripts/deploy.ps1",
    "deploy:prod": "npx vercel@latest --prod",
    "deploy:preview": "npx vercel@latest",
    "test": "echo \"Error: no test specified\" && exit 1",
    "verify": "npm run type-check && npm run lint && npm run build",
    "process:images": "node scripts/processAllImages.js hollywood",
    "process:images:teal": "node scripts/processAllImages.js tealOrange",
    "process:images:blockbuster": "node scripts/processAllImages.js blockbuster",
    "process:images:force": "node scripts/processAllImages.js hollywood --force",
    "watch:images": "node scripts/watchAndProcessImages.js"
  },
  "dependencies": {
    "@react-spring/web": "^9.7.3",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^9.0.0-rc.10",
    "@types/three": "^0.181.0",
    "@upstash/ratelimit": "^2.0.7",
    "@vercel/kv": "^3.0.0",
    "@visx/axis": "^3.5.0",
    "@visx/group": "^3.5.0",
    "@visx/scale": "^3.5.0",
    "@visx/shape": "^3.5.0",
    "chart.js": "^4.5.1",
    "critters": "^0.0.23",
    "d3": "^7.9.0",
    "deck.gl": "^9.0.35",
    "echarts": "^5.5.1",
    "echarts-for-react": "^3.0.2",
    "framer-motion": "^12.23.25",
    "gsap": "^3.12.5",
    "lightweight-charts": "^4.1.3",
    "mapbox-gl": "^3.6.0",
    "next": "^15.5.9",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.0",
    "recharts": "^2.12.7",
    "three": "^0.181.2",
    "web-vitals": "^4.2.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.1",
    "@tailwindcss/postcss": "^4",
    "@types/d3": "^7.4.3",
    "@types/lru-cache": "^7.10.9",
    "@types/mapbox-gl": "^3.4.0",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "lighthouse": "^12.8.2",
    "puppeteer": "^24.34.0",
    "sharp": "^0.33.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "nextjs",
    "react",
    "wordpress",
    "energy",
    "infrastructure",
    "three.js",
    "webgl"
  ],
  "author": "Emerson EIMS",
  "license": "MIT"
}
