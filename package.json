{
  "name": "emerson-eims",
  "version": "2.0.0",
  "description": "Emerson Energy Infrastructure Management System",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:3010": "next dev -p 3010",
    "dev:3020": "next dev -p 3020",
    "dev:debug": "set NODE_OPTIONS=--inspect && next dev",
    "build": "next build",
    "build:analyze": "set ANALYZE=true && next build",
    "build:debug": "next build --debug",
    "build:clean": "rimraf .next out && next build",
    "start": "next start",
    "start:3010": "next start -p 3010",
    "start:3020": "next start -p 3020",
    "cache:clear": "rimraf .next/cache",
    "cache:full-clear": "rimraf .next node_modules/.cache",
    "smoke:routes": "node ./scripts/probeRoutes.mjs",
    "smoke:json": "node ./scripts/probeJson.mjs",
    "smoke:missing": "node ./scripts/probeMissingRoutes.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "validate": "npm run lint && npm run typecheck",
    "clean": "rimraf .next out .turbo node_modules/.cache",
    "reset": "rimraf .next out .turbo node_modules/.cache node_modules package-lock.json && npm install",
    "inspect:port": "node ./scripts/inspectPort.mjs",
    "stop:port": "node ./scripts/stopPort.mjs",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:all": "npm run test:run && npm run test:e2e"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^9.0.0",
    "@sentry/nextjs": "^10.32.1",
    "@types/pg": "^8.16.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.0",
    "@vercel/kv": "^3.0.0",
    "autoprefixer": "^10.4.23",
    "chart.js": "^4.5.1",
    "critters": "^0.0.23",
    "echarts": "^5.6.0",
    "echarts-for-react": "^3.0.5",
    "framer-motion": "^12.24.0",
    "gsap": "^3.12.5",
    "lru-cache": "^11.2.4",
    "lucide-react": "^0.562.0",
    "mapbox-gl": "^3.17.0",
    "next": "^16.1.1",
    "next-intl": "^4.6.1",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.53.0",
    "tailwindcss": "^3.4.19",
    "three": "^0.181.2",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/lru-cache": "^7.10.9",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "jsdom": "^27.4.0",
    "sharp": "^0.33.5",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
